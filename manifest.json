{
  "manifest_version": 2,
  "name": "Memorizing ",
  "version": "1.0",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
     "matches": ["<all_urls>"],
     "js": ["jquery.js", "content.js"],
     "css": ["content.css"]
    }
  ]
}